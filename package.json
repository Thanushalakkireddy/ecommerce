{
  "name": "ecommerce",
  "version": "1.0.0",
  "description": "E-commerce application with API and client",
  "scripts": {
    "dev": "concurrently \"npm run api\" \"npm run client\"",
    "api": "cd api && npm run dev",
    "client": "cd client/ecommerce && npm run dev",
    "build": "cd client/ecommerce && npm run build",
    "start": "cd api && node index.js",
    "vercel-build": "npm run build"
  },
  "keywords": ["ecommerce", "react", "express", "mongodb"],
  "author": "Thanushalakkireddy",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}